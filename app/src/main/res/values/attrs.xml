<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="actionBarTitleStyle" format="reference" />
    <attr name="actionBarMenuType" format="reference" />
    <attr name="confirmations" format="reference" />
    <attr name="cn_background" format="reference" />

    <declare-styleable name="ConfirmationsWidgetStyle">
        <attr name="beadActiveColor" format="reference|color" />
        <attr name="beadInactiveColor" format="reference|color" />
        <attr name="beadDiameter" format="dimension" />
        <attr name="beadConnectorLength" format="dimension" />
        <attr name="stage" format="enum">
            <enum name="dropbitSent" value="0" />
            <enum name="addressReceived" value="1" />
            <enum name="broadcasting" value="2" />
            <enum name="pending" value="3" />
            <enum name="complete" value="4" />
        </attr>
        <attr name="configuration" format="enum">
            <enum name="dropbit" value="16" />
            <enum name="transaction" value="32" />
        </attr>
        <attr name="lastStepVector" format="reference" />
    </declare-styleable>
</resources>
